{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@internal/tsconfig/nextjs.json",
  "compilerOptions": {
    "jsx": "preserve",
    "paths": {
      "~/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.mjs",
    ".next/types/**/*.ts",
    "images.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
